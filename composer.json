{
    "name": "botman/tinker",
    "license": "MIT",
    "description": "BotMan tinker command for your Laravel BotMan project",
    "keywords": [
        "Bot",
        "BotMan",
        "Laravel",
        "Tinker"
    ],
    "homepage": "https://github.com/botman/tinker",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3|^8.4",
        "botman/botman": "~2.8",
        "clue/stdio-react": "^2.0",
        "illuminate/support": "^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/console": "^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.22.0||^9.0.0"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\Tinker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BotMan\\Tinker\\TinkerServiceProvider"
            ]
        }
    }
}
